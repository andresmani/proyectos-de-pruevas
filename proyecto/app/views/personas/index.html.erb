<%- model_class = Persona -%>
<div class="page-header">
  <h1>Personas</h1>
</div>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>identificacion</th>
      <th>email</th>
      <th>fch_ncto</th>
      <th>telefono</th> 
      <th>municipio</th>
      <th>acciones</th>

    </tr>
  </thead>
  <tbody>
     <%= render @personas %>
  </tbody>
</table>
<td><%= link_to 'Nuevo persona', new_persona_path, :remote => true, :class=>"btn btn-inverse" %></td>