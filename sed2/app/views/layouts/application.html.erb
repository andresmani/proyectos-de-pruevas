<html>
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title><%= content_for?(:title) ? yield(:title) : "SED" %></title>
		<meta name="description" content=" Sistema de Educacion Digital">
		<meta name="author" content="Andres Echeverri Arango">
		<%= stylesheet_link_tag "application", :media => "all" %>
		<%= javascript_include_tag "application" %>
		<%= csrf_meta_tags %>
		<%= yield(:head) %>
	</head>
	<body>
		<div class="tabbable navbar-inverse">
			<nav class="navbar-inner ">
				<div class="container ">
				<%= render 'layouts/navigation' %>
				</div>
			</nav>
		</div>	
		<br><br>
		<%= yield %>
	</body>
</html>